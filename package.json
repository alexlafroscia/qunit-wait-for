    "prepublishOnly": "microbundle"
  },
  "devDependencies": {
    "@types/qunit": "^2.9.0",
    "husky": "^7.0.1",
    "lint-staged": "^12.1.2",
    "microbundle": "^0.15.0",
    "prettier": "^3.1.1",
    "qunit": "^2.9.3",
    "standard-version": "^9.0.0",
    "testdouble": "^3.13.1",
    "testdouble-qunit": "^3.0.2",
    "typescript": "^4.0.2"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "lint-staged": {
    "*.{ts,json,md,yml}": "prettier --write"
  },
  "volta": {
    "node": "16.13.0",
    "yarn": "1.22.4"
  }
