{
  "name": "qunit-wait-for",
  "version": "2.0.0",
  "description": "Wait for a QUnit assertion",
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "exports": "dist/index.modern.js",
  "module": "dist/index.module.js",
  "unpkg": "dist/index.umd.js",
  "keywords": [
    "qunit",
    "qunit-plugin"
  ],
  "scripts": {
    "build": "microbundle",
    "fmt": "prettier --write .",
    "test": "qunit tests",
    "release": "standard-version"
  },
  "devDependencies": {
    "@types/qunit": "^2.9.0",
    "husky": "^5.0.9",
    "lint-staged": "^10.1.1",
    "microbundle": "^0.13.0",
    "prettier": "^2.0.2",
    "qunit": "^2.9.3",
    "standard-version": "^9.0.0",
    "testdouble": "^3.13.1",
    "testdouble-qunit": "^2.1.1",
    "typescript": "^4.0.2"
  },
  "standard-version": {
    "scripts": {
      "postchangelog": "prettier --write CHANGELOG.md"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,json,md,yml}": "prettier --write"
  },
  "volta": {
    "node": "12.16.1",
    "yarn": "1.22.4"
  }
}
