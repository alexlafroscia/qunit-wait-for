{
  "name": "qunit-wait-for",
  "version": "2.0.2",
  "description": "Wait for a QUnit assertion",
  "author": "Alex LaFroscia <alex@lafroscia.com>",
  "license": "MIT",
  "source": "./src/index.ts",
  "exports": {
    "require": "./dist/index.js",
    "default": "./dist/index.modern.js"
  },
  "main": "./dist/index.js",
  "module": "./dist/index.module.js",
  "unpkg": "./dist/index.umd.js",
  "files": [
    "dist"
  ],
  "keywords": [
    "qunit",
    "qunit-plugin"
